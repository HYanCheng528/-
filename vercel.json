{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "client.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/generate-name",
      "dest": "server.js"
    },
    {
      "src": "/client.js",
      "dest": "/client.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_KEY": "${API_KEY}"
  }
}